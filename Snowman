#include <iostream>
// this is the picture of a snowman
int main() {

std::cout << "          H H H H\n";
std::cout << "          H H H H\n";
std::cout << "        H H H H H H   \n";
std::cout << "         S  @ @  S\n";
std::cout << "        S    ^    S\n";
std::cout << "         S       S\n";
std::cout << "            SSS\n";
std::cout << "         S       S\n";
std::cout << "       S     O     S\n";
std::cout << " -----S      O       S-----  \n";
std::cout << "|    S       O        S    |\n";
std::cout << "     S       O        S    \n";
std::cout << "      S              S      \n";
std::cout << "        S           S  \n";
std::cout << "           S S S S  \n";
}
